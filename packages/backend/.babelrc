{
  "presets": [
    "@babel/preset-env",
    "@babel/typescript"
  ],
  "env": {
    "production": {
      "comments": false,
      "presets": [
        [
          "minify", { 
            "builtIns": false 
          }
        ]
      ]
    }
  },
  "plugins": [
    "@babel/transform-runtime",
    [
      "module-resolver", {
        "cwd": "./src",
        "alias": {
          "@config": "./config",
          "@controller": "./controller",
          "@events": "./events",
          "@middleware": "./middleware",
          "@models": "./models",
          "@routes": "./routes",
          "@schemas": "./schemas",
          "@services": "./services",
          "@structs": "./structs",
          "@utils": "./utils"
        }
      }
    ]
  ]
}