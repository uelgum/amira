{
  "presets": [
    "@babel/preset-env",
    "@babel/typescript"
  ],
  "plugins": [
    [
      "module-resolver", {
        "cwd": "./src",
        "alias": {
          "@config": "./config",
          "@api": "./api",
          "@loaders": "./loaders",
          "@models": "./models",
          "@schemas": "./schemas",
          "@services": "./services",
          "@structs": "./structs",
          "@utils": "./utils"
        }
      }
    ]
  ],
  "comments": false,
  "ignore": [ "./src/types" ]
}