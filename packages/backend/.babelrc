{
    "presets": [
        "@babel/preset-env",
        "@babel/typescript"
    ],
    "env": {
        "production": {
            "comments": false,
            "presets": [
                [
                    "minify", {
                        "builtIns": false
                    }
                ]
            ]
        }
    },
    "plugins": [
        [ "@babel/transform-runtime" ],
        [
            "module-resolver", {
                "cwd": "./src",
                "alias": {
                    "@middleware": "./middleware",
                    "@models": "./models",
                    "@services": "./services",
                    "@structs": "./structs",
                    "@utils": "./utils",
                    "@config": "./config",
                    "@routes": "./routes"
                }
            }
        ]
    ]
}